{
  "author": "bryn austin bellomy <bryn@signals.io>",
  "name": "hook.io-tropo",
  "description": "A hook for communicating with Tropo's WebAPI",
  "version": "0.0.1",
  "main" : "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/brynbellomy/hook.io-tropo.git"
  },
  "bin" : {
    "hookio-tropo" : "./bin/hookio-tropo"
  },
  "directories" : {
    "lib" : "./lib",
    "test" : "./spec"
  },
  "keywords" : [
    "hook.io","tropo","sms","text message","voice","voip"
  ],
  "scripts": {
    "test": "node_modules/.bin/vows --spec spec/*-spec.coffee",
    "watch" : "coffee -c -w  -o lib src"
  },
  "engines": {
    "node": "0.6.x"
  },
  "dependencies": {
    "hook.io" : "0.7.x",
    "pkginfo" : "0.x",
    "tropo-webapi" : "1.0.x",
    "async" : "0.1.x"
  },
  "devDependencies": {
    "should" : ">=0.2.1",
    "coffee-script" : ">= 1.1.2",         
    "vows" : ">= 0.5.9",
    "nock" : "*"
  },
  "bugs": { "url": "http://github.com/brynbellomy/hook.io-tropo/issues" },
  "licenses": [
    { "type": "MIT",
      "url": "http://github.com/brynbellomy/hook.io-tropo/raw/master/LICENSE"
    }
  ],
  "hook" : {
    "type" : "Tropo",
    "description" : "A hook for communicating with Tropo's WebAPI"
  }
}
